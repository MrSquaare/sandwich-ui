{
  "private": true,
  "name": "@sandwich-ui/react-storybook",
  "version": "0.2.5",
  "type": "module",
  "scripts": {
    "predev": "panda codegen",
    "dev": "storybook dev -p 6006 --no-open",
    "prebuild": "del-cli storybook-static/ && panda codegen",
    "build": "storybook build",
    "preview": "vite preview --outDir storybook-static/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "del-cli storybook-static/ node_modules/.cache/"
  },
  "dependencies": {
    "@sandwich-ui/core": "workspace:*",
    "@sandwich-ui/react": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.53.6",
    "@sandwich-ui/eslint-config": "workspace:*",
    "@sandwich-ui/eslint-config-react": "workspace:*",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.1",
    "eslint-plugin-storybook": "^0.12.0",
    "postcss": "^8.5.3",
    "del-cli": "^6.0.0",
    "storybook": "^8.6.12",
    "typescript": "~5.8.3",
    "vite": "^6.3.4"
  }
}
