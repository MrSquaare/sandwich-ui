{
  "name": "@mrsquaare/sandwich-ui",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/sandwich-ui.cjs",
  "module": "./dist/sandwich-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./panda-preset": {
      "require": "./dist/panda-preset/index.cjs",
      "import": "./dist/panda-preset/index.js",
      "types": "./dist/panda-preset/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "predev": "panda codegen",
    "dev": "vite build --watch",
    "prebuild": "rimraf dist/ && panda codegen",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rimraf dist/ src/styled-system/ node_modules/.cache/"
  },
  "dependencies": {
    "@ark-ui/react": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.41.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  }
}
